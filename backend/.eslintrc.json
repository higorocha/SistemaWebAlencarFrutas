{
  "extends": [
    "@nestjs/eslint-config",
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-unused-vars": "warn",
    "@typescript-eslint/no-floating-promises": "warn"
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  }
} 